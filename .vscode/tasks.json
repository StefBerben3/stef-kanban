{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Start front-end",
      "type": "npm",
      "options": {
        "cwd": "${workspaceFolder}/kanban-frontend"
      },
      "script": "start"
    },
    {
      "label": "Start back-end",
